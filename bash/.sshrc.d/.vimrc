syntax on
set tabstop=2
set number
set rnu
set background=dark
set t_Co=256
filetype plugin indent on
set expandtab
set shiftwidth=2
set softtabstop=2
set hlsearch

nnoremap <F2> gT
nnoremap <F3> gt
" Disable pleb keys
noremap <Up> <NOP>
noremap <Down> <NOP>
noremap <Left> <NOP>
noremap <Right> <NOP>

" Set filetypes
" autocmd BufNewFile,BufRead *.sls set filetype=yaml
